{
  "mcpServers": {
    "supabase": {
      "command": "npm",
      "args": ["run", "dev"],
      "cwd": "./servers/supabase-mcp",
      "env": {
        "SUPABASE_URL": "https://YOUR-PROJECT.supabase.co",
        "SUPABASE_SERVICE_ROLE_KEY": "YOUR_SERVICE_ROLE_KEY",
        "SUPABASE_MCP_WRITE_TABLES": "community_posts,community_comments"
      }
    }
  }
}

